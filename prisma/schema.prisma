// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = "mongodb://localhost:27017/workout-app"
}

model users {
  id          String    @id @map("_id")
  username    String
  email       String
  password    String
  meoscycles  String[]
  created_at  String
  updated_at  String?
}
